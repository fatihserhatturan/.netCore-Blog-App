@model IEnumerable<EntityLayer.Concrete.Comment>

<div class="pt-5">
    <h3 class="mb-5">Yorumlar</h3>
    <ul class="comment-list">
        <li class="comment">

            @foreach(var item in Model){
            <div class="comment-body">
                <h3>@item.UserName</h3>
                <p>@item.CommentText</p>
            </div>
            }
    </ul>

</div>
